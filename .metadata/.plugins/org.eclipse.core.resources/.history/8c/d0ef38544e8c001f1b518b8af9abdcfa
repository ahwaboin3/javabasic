package c18;

import java.time.LocalTime;

public class LocalTimeDemo2 {
	public static void main(String[] args) {
		//PC방의 PC 이용 시작 시각
		LocalTime start =LocalTime.of(14, 24,35);
		System.out.println("PC 이용 시작 시각 :"+start);
		
		//PC방의 PC  이용 종료 시각
		LocalTime end=LocalTime.of(17, 31,19);
	}

}





