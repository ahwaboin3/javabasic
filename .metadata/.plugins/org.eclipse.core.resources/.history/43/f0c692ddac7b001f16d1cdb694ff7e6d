package c03;

public class C03_01_04 {

	public static void main(String[] args) {
		//논리 연산자
		int num1=42;
		System.out.println(num1>10&&num1<50);
		//논리곱(and) - &&, & - ~와
		//연산식 | 결과
		//true && true | true
		//true && false | false
		//false && true | false
		//false && false | false
		//피연산자 모두가 true일 경우에만 연산 결과가 true
		
		//논리합(OR) - ||, | - ~또는
		//연산식 | 결과
		//true || true => true
		//true || false => true
		//false || true => true
		//false || false => false
		
		
	}

}
